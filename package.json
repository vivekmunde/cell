  "peerDependencies": {
    "pusu": "1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "babel-jest": "^25.5.1",
    "jest": "^25.5.4",
    "pusu": "1.2.2",
